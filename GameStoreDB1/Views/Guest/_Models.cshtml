
@model IEnumerable<GameStoreDB1.Model>

@{
    ViewBag.Title = "_FilterSystems";
}
<div>
    @foreach (var item in Model)
    {
        <div>
            <input value="@item.ModelId" type="checkbox" checked="true" onclick="SelectSystem(this)" /> @item.GameSystem.SystemName - @item.ModelName
        </div>
    }
</div>
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

}
